{
  "name": "yank-note-extension-math",
  "version": "0.2.2",
  "displayName": "数学扩展",
  "description": "Yank Note 数学扩展",
  "author": "Mirion",
  "license": "MIT",
  "homepage": "https://github.com/MirionQs/yank-note-extension-math",
  "main": "./dist/index.js",
  "icon": "./icon.png",
  "engines": {
    "node": ">=18.20.2",
    "yank-note": ">=3.73.1"
  },
  "themes": [
    {
      "name": "香蕉空间",
      "css": "./themes/banana-space.css"
    },
    {
      "name": "多彩笔记",
      "css": "./themes/colorful-note.css"
    }
  ],
  "files": [
    "*.md",
    "dist",
    "themes",
    "icon.png"
  ],
  "scripts": {
    "link-extension": "link-yn-extension",
    "unlink-extension": "link-yn-extension --unlink",
    "dev": "vite build --watch",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "vue-tsc --noEmit && yarn run eslint '{src,__tests__}/**/*.{js,ts,tsx,vue}'",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@yank-note/runtime-api": "^3.73.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "terser": "^5.31.3",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vue": "^3.2.33",
    "vue-tsc": "^0.34.11"
  }
}